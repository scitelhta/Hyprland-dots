(defwidget metric [label value onchange]
	(box :orientation "h"
		:class "metric"
		:space-evenly false

		(box :class "label" label value)
		(circular-progress
		:start-at 0

			:value value
		)
		(progress :value value :orientation 'h')
		;;(scale :min 0
          ;; :max 101
           ;;:active {onchange != ""}
           ;;:value value
           ;;:onchange onchange)
	)
)


(defwidget imetric [image value ?onclick ?onmiddleclick ?onrightclick ?onscroll ?hide]
	(eventbox
:visible {hide!=true}
;;		:timeout "20s"
		:onscroll onscroll
		:onmiddleclick onmiddleclick
		:onclick onclick
		:onrightclick onrightclick
		(box :orientation "h"
			:class "metric"
			:space-evenly false


			(image
			:class "imagemetric"
			:path "${'/home/'+username+'/.config/eww/assets/'+image}" :image-width 30
:image-height 30)
			(box :class "label"  value)

		)
	)

)

